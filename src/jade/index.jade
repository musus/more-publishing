doctype html
html(lang="ja")
	head
		title More Publishing by Susumu Seino
		link(rel='stylesheet', href='./assets/css/app.css')
		meta(name='viewport', content='width=device-width, initial-scale=1')
		meta(charset='utf-8')
		script(src='//ajax.googleapis.com/ajax/libs/jquery/1.11.1/jquery.min.js')
		script(src='src/js/app.js')
		link(rel='stylesheet', href='//maxcdn.bootstrapcdn.com/bootstrap/4.0.0/css/bootstrap.min.css')
		script(src='//maxcdn.bootstrapcdn.com/bootstrap/4.0.0/js/bootstrap.min.js')
	body
		.page
			.box.red
				.typewriter
					h1 More Publishing
					a.typewrite(href='', data-period='2000', data-type='[ "     ", "Hi, I am  Susumu.", "I am Creative.", "I Love to Develop.", "I Love WordPress." ]')
					span.wrap
					ul.menu
						li.menu__item
							a(href='#') CODE
						li.menu__item
							a(href='#') PHOTO
						li.menu__item
							a(href='#') TRIP
				#large-header.large-header
					canvas#demo-canvas
			.box.street.skill
				.row
					.col-lg-6.col-sm-12
						.block.cf
							.block-half
								h1 Susumu Seino
							.text-left
								p WondPressやCMSを活用したデジタルマーケティングのハブの企画・開発・運用をおこっています。
							.block-half
								img.icon(src='/assets/img/icon.jpg')
					.col-lg-6.col-sm-12.skill-bar-wrapper
						h1.h1--skills Skills
						.text-left
							// Digital Marketing
							| Digital Marketing
							span(style='float:right;') 75%
							.skillbar-container.clearfix(data-percent='75%')
								.skills(style='background: white;')
							// WordPress
							| WordPress
							span(style='float:right;') 80%
							.skillbar-container.clearfix(data-percent='80%')
								.skills(style='background: white;')
							// WooCommerce
							| WooCommerce
							span(style='float:right;') 70%
							.skillbar-container.clearfix(data-percent='70%')
								.skills(style='background: white;')
							// Code
							| Code ( HTML / CSS )
							span(style='float:right;') 80%
							.skillbar-container.clearfix(data-percent='80%')
								.skills(style='background: white;')
							// Program
							| Program ( PHP / JavaScript )
							span(style='float:right;') 70%
							.skillbar-container.clearfix(data-percent='70%')
								.skills(style='background: white;')
							// Design
							| Design
							span(style='float:right;') 55%
							.skillbar-container.clearfix(data-percent='55%')
								.skills(style='background: white;')
							// Photography
							| Photography
							span(style='float:right;') 50%
							.skillbar-container.clearfix(data-percent='50%')
								.skills(style='background: white;')
							// Videograph
							| Videograph
							span(style='float:right;') 55%
							.skillbar-container.clearfix(data-percent='55%')
								.skills(style='background: white;')
							// Drone
							| Drone (DJI MavicPro)
							span(style='float:right;') 70%
							.skillbar-container.clearfix(data-percent='90%')
								.skills(style='background: white;')
							// Football
							| Football
							span(style='float:right;') 60%
							.skillbar-container.clearfix(data-percent='60%')
								.skills(style='background: white;')
							| Freedom
							span(style='float:right;') 90%
							.skillbar-container.clearfix(data-percent='90%')
							// Freedom
							| Freedom
							span(style='float:right;') 100%
							.skillbar-container.clearfix(data-percent='100%')
								.skills(style='background: white;')
		include parts/_footer.jade